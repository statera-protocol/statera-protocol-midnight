    "build": "rm -rf dist && tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@statera/ada-statera-protocol": "workspace:*",
    "pino": "^8.16.1",
    "uuid": "^11.1.0",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@types/ws": "^8.5.9",
    "allure-commandline": "^2.28.0",
    "allure-jest": "^2.15.1",
    "allure-js-commons": "^2.15.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  }
}
